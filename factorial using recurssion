#include <stdio.h> 
int factorial(int n) 
{ 
{ 
if (n < 0) 
return -1;  
}  
else if (n == 0)  
{ 
return 1 
} 
else  
{ 
} 
return n * factorial(n - 1);  
} 
int main()  
{ 
int n; 
printf("Enter a appositive number: "); 
scanf("%d", &n); 
int result = factorial(n); 
if (result == -1) 
{ 
printf("Factorial is not defined for negative numbers.\n"); 
} 
else  
{ 
} 
printf("Factorial of %d is %d\n", n, result); 
} 
return 0;
